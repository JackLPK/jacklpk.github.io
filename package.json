{
  "name": "my-frontend",
  "version": "0.1.0",
  "private": true,
  "homepage": "pkljack.github.io/",
  "scripts": {
    "dev": "next dev --hostname localhost --port 3333",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "preexport": "npm run build",
    "export": "next export",
    "export:win": "npm run export && type NUL > ./out/.nojekyll",
    "export:linux": "npm run export && touch ./out/.nojekyll",
    "local": "http-server out -a localhost",
    "deploy": "gh-pages --dotfiles --dist out"
  },
  "dependencies": {
    "@svgr/webpack": "^6.5.1",
    "@types/node": "18.11.17",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.9",
    "eslint-config-next": "13.0.7",
    "next": "13.0.7",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "semantic-ui-css": "^2.5.0",
    "semantic-ui-react": "^2.1.4",
    "typeit-react": "^2.6.1",
    "typescript": "4.9.4"
  },
  "devDependencies": {
    "eslint": "^8.30.0",
    "gh-pages": "^4.0.0",
    "sass": "^1.57.1"
  }
}
