    "local": "http-server out -a localhost",
    "deploy": "gh-pages --dotfiles --dist out"
  },
  "dependencies": {
    "@svgr/webpack": "^6.2.1",
    "bootstrap-icons": "^1.8.3",
    "next": "12.2.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typeit-react": "^2.5.4"
  },
  "devDependencies": {
    "eslint": "8.19.0",
    "eslint-config-next": "12.2.2",
    "gh-pages": "^4.0.0"
  }
}
